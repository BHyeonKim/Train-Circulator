package trainpkg;

public class Route {
	private int start;//출발역
	private int end;//도착역
	private int time;//시간
	private String dir;//상행 or 하행
	
	
	public Route(int start, int end, String dir) {
		this.setStart(start);
		this.setEnd(end);
		this.setDir(dir);
	}
	
	public int getStart() {
		return start;
	}
	public int getEnd() {
		return end;
	}
	public String getDir() {
		return dir;
	}
	
	public void setStart(int start) {
		this.start = start;
	}
	public void setEnd(int end) {
		this.end = end;
	}
	public void setDir(String dir) {
		this.dir = dir;
	}

}
