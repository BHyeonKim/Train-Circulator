package trainpkg;

public class Route {
	private int end;//도착역
	private int time;//시간
	private String dir;//상행 or 하행
	
	
	public Route(int end, String dir, int time) {
		this.setEnd(end);
		this.setDir(dir);
		this.setTime(time);
	}
	
	public int getEnd() {
		return end;
	}
	public String getDir() {
		return dir;
	}
	public int getTime() {
		return time;
	}
	
	public void setTime(int time) {
		this.time = time;
	}
	public void setEnd(int end) {
		this.end = end;
	}
	public void setDir(String dir) {
		this.dir = dir;
	}

}
